# aspose-words安装到本地仓库
mvn install:install-file -Dfile=src/main/resources/lib/aspose-words-21.1-jdk17-cracked.jar -DgroupId=com.aspose -DartifactId=aspose-words -Dversion=21.1.0 -Dpackaging=jar -Dclassifier=jdk17

# 卸载
mvn dependency:purge-local-repository -DmanualInclude="com.aspose:aspose-words"
mvn dependency:purge-local-repository -DreResolve=false
